<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Altidude.net is a tool for creating more fun altitude/elevation/stage profiles from your training data">
    <meta name="keywords" content="elevation profile,altitude profile,stage profile,strava,tcx,training data">
    @RenderSection("MetaTags", required: false)
    <title>@ViewBag.Title - Altidude.net</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-86170840-1', 'auto');
  ga('send', 'pageview');

    </script>    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink("Altidude.net", "index", "home", new { area = "" }, new { @class = "navbar-brand" })<small>BETA</small>*@
                <a class="navbar-brand" href="@Url.Action("index", "home")">
                    Altidude.net&nbsp;<small>BETA</small>
                    @*<img src="@Url.Content("~/Images/logo_test.png")" width="110" height="25"/>*@
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Start", "index", "home")</li>
                    <li>@Html.ActionLink("Create", "create", "profile")</li>
                    <li>@Html.ActionLink("Profiles", "index", "profile")</li>
                    <li>@Html.ActionLink("Contact", "contact", "home")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Altidude.net</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


    @Scripts.Render("~/Scripts/snap.svg.js")
    @Scripts.Render("~/Scripts/angular.min.js")
    @Scripts.Render("~/Scripts/ui-bootstrap-tpls-2.1.4.min.js")
    @Scripts.Render("~/bundles/canvg")
    @Scripts.Render("~/Scripts/svg_todataurl.js")


    @Scripts.Render("~/Scripts/ng-file-upload.js")
    @Scripts.Render("~/Scripts/altidude.js")


    @RenderSection("scripts", required: false)
</body>
</html>
